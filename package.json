{
  "name": "wedding-quiz-app",
  "version": "1.0.0",
  "engines": {
    "node": "18.x"
  },
  "description": "結婚式用クイズアプリ",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
